
material Box {
	shader Default:Default.Deferred-1 {Data}/Shaders/Deferred.nfx
	shader_options none
	param_value diffuseAndGloss .3 .3 .3 .5
}

material BoxTextured {
	shader Default:Default.Deferred-2 {Data}/Shaders/Deferred.nfx 
	shader_options USE_DIFFUSE_SAMPLER
	param_value diffuseAndGloss .3 .3 .3 .5
	param_value diffuseMap Default:Default.BoxTexture {Data}/Textures/Box.jpg 
}
