
material Default:Default.Box {	
	shader {Data}/Shaders/Deferred.nfx Default:Default.Deferred
	layer normal
	param_value diffuseAndGloss ".3 .3 .3 .5"
}

material Default:Default.BoxTextured {
	shader {Data}/Shaders/Deferred.nfx
	layer normal 
	enable_options USE_DIFFUSE_SAMPLER
	param_value diffuseAndGloss ".3 .3 .3 .5"
	param_value diffuseMap {Data}/Textures/Box.jpg BoxTexture 
}
